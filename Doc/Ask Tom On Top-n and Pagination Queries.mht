// MW: this is only used for v1 cookies
function private_UCMCookieDecode(value) {
	var asciiArray =3D " =
!\"#$&'()*+,-./0123456789:;<=3D>?@ABCDEFGHIJKLMNOPQRSTUVWXYZ[]^_`abcdefgh=
ijklmnopqrstuvwxyz{|}~.";
	var urldecodevalue =3D unescape(value);
	var ucmdecodevalue =3D '';
	var ch =3D '';

	for (i=3D0; i<urldecodevalue.length; i++) {

		ch =3D urldecodevalue.charAt(i)
		j =3D asciiArray.indexOf(ch);
		if (j !=3D -1) {

			j +=3D2;
			if( j > ( asciiArray.length - 1 ) ) {
				j -=3D asciiArray.length;
			}
			ucmdecodevalue +=3D asciiArray.charAt( j );
		}
		else {
			ucmdecodevalue +=3D ch;
		}
	}
	return ucmdecodevalue;
}

function getCookieData(label) {
	var labelLen =3D label.length
	var cLen =3D document.cookie.length
	var i =3D 0
	var cEnd
	while (i < cLen) {
	var j =3D i + labelLen
	if (document.cookie.substring(i,j) =3D=3D label) {
		cEnd =3D document.cookie.indexOf(";",j)
		if (cEnd =3D=3D -1) {
			cEnd =3D document.cookie.length
		}
	j++;
	return unescape(document.cookie.substring(j,cEnd))
	}

	i++
	}

	return "";
}

