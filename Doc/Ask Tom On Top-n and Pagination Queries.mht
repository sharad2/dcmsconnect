	var exp =3D new Date();
	var cookieval =3D name + "=3D" + escape(value) + "; ";
	var date =3D exp.getTime();
	if (time > 0) {
      	if (ttype =3D=3D "year") exp.setTime(date + (time * year));
		else if (ttype =3D=3D "day") exp.setTime(date + (time * day));
		else if (ttype =3D=3D "hour") exp.setTime(date + (time * hour));

      	cookieval +=3D "expires=3D" + exp.toGMTString();
	}
    cookieval +=3D "; domain=3D.oracle.com; path=3D/";
	document.cookie =3D cookieval;
