@model DcmsMobile.PieceReplenish.ViewModels.ViewModelBase
@*        All pages which use this layout must ensure that the script PieceReplenish.partial.js is included.*@
<!DOCTYPE html>
<!--
    Design by Free CSS Templates
    http://www.freecsstemplates.org
    Released for free under a Creative Commons Attribution 2.5 License
-->
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <title>@ViewBag.Title</title>
    <meta http-equiv="X-UA-Compatible" content="IE=9" />
    <meta name="keywords" content="" />
    <meta name="description" content="" />
    <link href="@Links_PieceReplenish.Areas.PieceReplenish.Content.PieceReplenish_all_css" rel="stylesheet" type="text/css" />
    <link href="@Links_PieceReplenish.Content.themes.Start1_10_0.jquery_ui_css" rel="stylesheet" type="text/css" />
</head>
<body>
    @if (Model != null)
    {
        @Html.Partial(MVC_PieceReplenish.PieceReplenish.Shared.Views._soundPartial, Model.Sound)
    }
    <div id="outer" class="ui-widget">
        <div id="header" class="ui-widget-header">
            <span style="font-size: medium;">@ViewBag.Title</span>
        </div>
        <div id="menu">
            <ul>
                <li class="banner"><span>DCMS Connect - Piece Replenishment</span></li>
                <li><a href="@Url.Content("~")">DCMS Connect Home</a></li>
                @if (ViewContext.HttpContext.User.Identity.IsAuthenticated)
                {
                    <li>Welcome @ViewContext.HttpContext.User.Identity.Name</li>
                }
                @if (ViewContext.HttpContext.Trace.IsEnabled)
                {
                    <li><a href="@Url.Content("~/trace.axd")" target="trace">Trace</a> </li>
                }
                <li><a href="@Url.Content(Links_PieceReplenish.Areas.PieceReplenish.Content.Replenishment_pptx)">Presentation</a> </li>
            </ul>
        </div>
        <div id="content">
            <div id="secondaryContent">
                @RenderSection("sidebar", false)
            </div>
            <div id="primaryContentContainer">
                <div id="primaryContent">
                    @RenderBody()
                </div>
                <div id="footer">
                    <p>
                        Copyright &copy; 2012 Eclipse Systems <a href="http://www.eclsys.com" target="_blank">http://www.eclsys.com</a>
                    </p>
                </div>
            </div>

        </div>

    </div>

    <script src="@Links_PieceReplenish.Scripts.jquery_2_1_1_js" type="text/javascript"></script>

    <script>$.uiBackCompat = false;</script>
    <script src="@Links_PieceReplenish.Scripts.jquery_ui_1_10_0_js" type="text/javascript"></script>
    @RenderSection("scripts", false)
</body>
</html>
